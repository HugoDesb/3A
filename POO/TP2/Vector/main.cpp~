#include <iostream>
#include "vector.h"
using namespace std;

int main(int argc, char const *argv[])
{
	Vector v1(2);
	Vector v2(3);
	Vector v3();

	Vector15 v15;

	v1.afficher();

	v1 = v2;

	v1.afficher();

	v1.adjust_size(5);

	v1.set_item(3, 7);
	cout << v1.get_item(3) << endl;

	v1.afficher();

	v1.afficher_acces();

	v1[0] = 2;
	v1[1] = 3;

	cout << "v1[1] = " << v1[1] << endl;

	v1.afficher();

	cout << "Vector15" << endl;

	v15.afficher();

	//v15.adjust_size(5);

	return 0;
}
